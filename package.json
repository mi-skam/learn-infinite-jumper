{
  "name": "learn-infinite-jumper",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gh-pages": "cross-env DEPLOY_ENV=gh-pages vite build",
    "preview": "npm run build && vite preview",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "gh-deploy": "npm run build:gh-pages && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@new-objects/eslint-config": "^0.0.3",
    "@new-objects/prettier-config": "^0.0.2",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.3",
    "vite": "^5.0.5",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "phaser": "^3.70.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/new-objects/learn-infinite-jumper"
  },
  "bugs": {
    "url": "https://github.com/new-objects/learn-infinite-jumper/issues"
  }
}